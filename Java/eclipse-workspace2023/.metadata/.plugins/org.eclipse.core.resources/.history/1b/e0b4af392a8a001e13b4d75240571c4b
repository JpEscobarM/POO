public class Horario implements Serializable{
	
	Date data;
	List <String> horas = new ArrayList<>();
	Filme filme;
	

	public Horario() {
		
	}
	public void horariosPadrao() {
		hora.add("13:00");
		hora.add("14:30");
		hora.add("16:30");
		hora.add("18:00");
		hora.add("19:30");
		hora.add("21:00");
		hora.add("22:30");
		
		int i = 0;
		for(i=0; horas.size(); i++)){
			
		}
	}
	
	public Date getData() {
		return data;
	}
	public void setData(Date data) {
		this.data = data;
	}
	
	public Filme getFilme() {
		return filme;
	}
	public void setFilme(Filme f) {
		this.filme = f;
	}
	
	
	
	
	
}
