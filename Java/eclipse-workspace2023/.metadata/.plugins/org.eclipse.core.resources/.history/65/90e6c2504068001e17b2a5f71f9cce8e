
public class Turma {
	private String codigo;
	private Professor professor;
	private Disciplina disciplina;
	private int alunosAprovados;
	
	public Turma( ) {

	}
	
	public Turma(String c, Professor p, Disciplina d ) {
		this.setCodigo(c);
		this.setProfessor(p);
		this.setDisciplina(d);
	}
	
	public void regAluno() {
		
	
	}
//G and S:
	public String getCodigo() {
		return codigo;
	}

	public void setCodigo(String codigo) {
		this.codigo = codigo;
	}

	public Professor getProfessor() {
		return professor;
	}

	public void setProfessor(Professor professor) {
		this.professor = professor;
	}

	public Disciplina getDisciplina() {
		return disciplina;
	}

	public void setDisciplina(Disciplina disciplina) {
		this.disciplina = disciplina;
	}

	public int getAlunosAprovados() {
		return alunosAprovados;
	}

	public void setAlunosAprovados(int alunosAprovados) {
		this.alunosAprovados = alunosAprovados;
	}
}
